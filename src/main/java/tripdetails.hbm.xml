<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.grandcircus.planit.tripDetails" table="tripDetails">
			<id name="eventID" column="eventID" type="string"></id>
	<property name="tripID" column="tripID" type="int"> </property>
		<property name="typeOfEvent" column="typeOfEvent" type="string"></property>
		<property name="dateOfEvent" column="dateOfEvent" type="string"></property>
	</class>
</hibernate-mapping> 